    "karma-phantomjs-launcher": "^1.0.4",
    "node-sass": "^4.9.3",
    "protractor": "^5.4.0",
    "ts-node": "~3.2.0",
    "tslint": "^5.11.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~2.9.2"
  },
  "prettier": {
    "singleQuote": true
  }
