{
  "name": "kyma-console",
  "version": "0.6.1",
  "license": "MIT",
  "scripts": {
    "start": "webpack-cli serve --mode=development --config=webpack-generateConfig.config.js",
    "start:kyma": ". ../scripts/load-cluster-config.sh && npm run start",
    "build": "npm run buildConfig",
    "test": "../node_modules/.bin/jest",
    "buildConfig": ". ../scripts/load-cluster-config.sh && webpack --mode production --config webpack-generateConfig.config",
    "buildConfig:watch": ". ../scripts/load-cluster-config.sh && webpack -d --config webpack-generateConfig.config --watch"
  },
  "private": true,
  "dependencies": {
    "@luigi-project/core": "^1.7.1",
    "@luigi-project/plugin-auth-oidc": "1.8.1",
    "copy-webpack-plugin": "^5.0.0",
    "file-saver": "2.0.2",
    "json-url": "^2.6.0"
  },
  "devDependencies": {
    "@webpack-cli/serve": "^1.3.0",
    "babel-loader": "^8.0.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.js$"
  }
}
