{
  "name": "kyma-console",
  "version": "0.6.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "echo `` > src/assets/config/config.js && ng serve --disable-host-check --host console-dev.kyma.local",
    "start:kyma": "npm run copy-config && ng serve --disable-host-check --host console-dev.kyma.local",
    "start:kyma:api": "npm run copy-config && ng serve --disable-host-check --host console-dev.kyma.local --configuration api-local",
    "build": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint",
    "copy-config": "cp ../config.js src/assets/config/config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.4",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.1.4",
    "@angular/http": "^6.1.4",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/router": "^6.1.4",
    "@kyma-project/y-generic-list": "^0.5.5",
    "@types/async": "^2.0.49",
    "rxjs": "^6.3.1",
    "angular-oauth2-oidc": "^4.0.2",
    "angular-sortablejs": "^2.6.0",
    "core-js": "^2.5.7",
    "file-saver": "^1.3.8",
    "js-yaml": "^3.12.0",
    "jsoneditor": "^5.24.3",
    "lodash": "^4.17.10",
    "ng-click-outside": "^4.0.0",
    "ngx-clipboard": "^11.1.7",
    "sortablejs": "^1.7.0",
    "time-ago-pipe": "^1.3.2",
    "tippy.js": "^2.5.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "ajv": "^6.5.3",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "~1.1.2",
