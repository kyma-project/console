{
  "name": "kyma-console",
  "version": "0.6.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "echo `` > src/assets/config/config.js && concurrently \"npm run buildConfig:watch\" \"ng serve --disable-host-check --host console-dev.kyma.local\"",
    "start:kyma": "npm run copy-config && concurrently \"npm run buildConfig:watch\" \"ng serve --disable-host-check --host console-dev.kyma.local\"",
    "start:kyma:api": "npm run copy-config && concurrently \"npm run buildConfig:watch\" \"ng serve --disable-host-check --host console-dev.kyma.local --configuration api-local\"",
    "build": "npm run buildConfig && ng build --prod",
    "test": "ng test",
    "test:single": "ng test --watch=false",
    "lint": "ng lint --tslint-config tslint.json",
    "copy-config": "cp ../config.js src/assets/config/config.js",
    "buildConfig": "webpack --config webpack-generateConfig.config",
    "buildConfig:watch": "webpack --config webpack-generateConfig.config --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.4",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.1.4",
    "@angular/http": "^6.1.4",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/router": "^6.1.4",
    "@kyma-project/luigi-client": "^0.4.8",
    "@kyma-project/luigi-core": "^0.4.8",
    "@types/async": "^2.0.49",
    "angular-sortablejs": "^2.6.0",
    "core-js": "^2.5.7",
    "file-saver": "^1.3.8",
    "fiori-fundamentals": "^1.4.1",
    "fundamental-ngx": "0.0.29-beta",
    "js-yaml": "^3.12.0",
    "jsoneditor": "^5.24.3",
    "lodash": "^4.17.11",
    "ng-click-outside": "^4.0.0",
    "ngx-clipboard": "^11.1.7",
    "rxjs": "^6.3.1",
    "sortablejs": "^1.7.0",
    "time-ago-pipe": "^1.3.2",
    "tippy.js": "^2.5.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "7.2.0",
    "@angular-builders/dev-server": "7.1.1",
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/core": "0.6.8",
    "@angular-devkit/schematics": "^0.6.8",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.2.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~6.0.60",
    "ajv": "^6.5.3",
    "babel-loader": "^8.0.4",
    "codelyzer": "~4.4.4",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.0",
