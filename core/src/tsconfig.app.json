{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": ["node"],
    "paths": {
      "shared/*": ["app/shared/*"],
      "namespaces/*": ["app/content/namespaces/*"]
    },
    "lib": ["es2017", "dom", "esnext.asynciterable", "es2018.promise"]
  },
  "include": ["**/*.ts"],
  "exclude": ["test.ts", "**/*.spec.ts"]
}
